package entities;

public class Pensionato {

	
	public Integer room;
	public Integer rent;


	public Integer getRent() {
		return rent;
	}


	public void setRent(Integer rent) {
		this.rent = rent;
	}


	public Pensionato(Integer room) {
		this.room = room;
	}


	public Pensionato() {
		// TODO Auto-generated constructor stub
	}


	public Integer getRoom() {
		return room;
	}


	public void setRoom(Integer room) {
		this.room = room;
	}
	
	
	
	
}
