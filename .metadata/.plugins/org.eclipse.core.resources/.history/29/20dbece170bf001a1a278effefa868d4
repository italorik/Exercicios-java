package entities;

public class CurrencyConverter {

	public double pricedollar;
	public double quantitydollar;
	public double dollar;
	public double result;
	
	public double result ( ) {
		
		return pricedollar * quantitydollar;
				 
	}
	public void addIof(double result) {
		double iof = result * 0.06;
		 System.out.println(iof);
	}
}

