package entities;

public class CurrencyConverter {

	public double pricedollar;
	public double quantitydollar;
	public double iof;
	public double dollar;
	public double result;
	
	public double converterToDollar ( ) {
		
		return result = pricedollar * quantitydollar;
	}
	public static double addIof(double result ) {
		
		return iof = result * 0.06;
	}
}

